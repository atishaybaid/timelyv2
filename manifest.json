{
"manifest_version": 2,
  "name": "timelyv2",
  "permissions":["history","http://api.wunderground.com/api/"],
  "version": "2.0",
  "background": {
    "scripts": ["app/bower_components/angular/angular.js"]
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "chrome_url_overrides" : {
    "newtab": "app/src/index.html"
  }
}